<AnchorPane fx:id="bgAnchor" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.apis.gui.controller.transaction.TransactionNativeDetailsContentsController">
   <children>
      <GridPane fx:id="gridPane" minHeight="-Infinity" prefHeight="40.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <columnConstraints>
            <ColumnConstraints percentWidth="17.7" />
            <ColumnConstraints hgrow="SOMETIMES" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <Label fx:id="contentsHeader" styleClass="font_semibold_12" text="title" textFill="#2b2b2b">
               <GridPane.margin>
                  <Insets left="10.0" />
               </GridPane.margin>
            </Label>
            <HBox fx:id="contentsBodyList" spacing="4.0" styleClass="font_regular_12" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
               <children>
                  <AnchorPane>
                     <children>
                        <Label fx:id="contentsBody" text="label" textFill="#2b2b2b" wrapText="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                     </children>
                  </AnchorPane>
               </children>
               <GridPane.margin>
                  <Insets right="8.0" />
               </GridPane.margin>
            </HBox>
            <TextArea fx:id="textArea" editable="false" styleClass="scroll_pane_transparent" visible="false" wrapText="true" GridPane.columnIndex="1" />
         </children>
      </GridPane>
   </children>
   <stylesheets>
      <URL value="@../css/common.css" />
      <URL value="@../css/scroll.css" />
   </stylesheets>
</AnchorPane>
